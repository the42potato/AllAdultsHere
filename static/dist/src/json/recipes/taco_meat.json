    "meal_type": ["lunch", "dinner"],
    "prep_type": "stovetop",
    "total_prep_time_minutes": 25,
    "ingredient_prep_time_minutes": 10,
    "cook_time_minutes": 15,
    "servings": 16,
    "ingredients": [
        "1 pound lean ground beef",
        "1 white onion",
        "1 28 ounce can diced tomatoes",
        "1 15-16 ounce can refried beans",
        "3 cloves garlic",
        "1.5 teaspoons chili powder",
        "1.5 teaspoons cumin",
        "1 teaspoon salt"
    ],
    "equipment": [
        "Skillet",
        "Spatula or spoon"
    ],
    "instructions": [
        [
            {"text": "Dice garlic and onion."}
        ],
        [
            {"text": "Brown ground beef, garlic, and onion in a skillet. Add in seasonings and cook for a couple minutes until fragrant."}
        ],
        [
            {"text": "Stir in tomatoes and refried beans. Cook slowly until heated through and bubbly."}
        ]
    ]
}