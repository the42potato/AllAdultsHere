{
    "title": "Zucchini Tots",
    "meal_type": ["breakfast", "lunch", "dinner"],
    "prep_type": "oven",
    "total_prep_time_minutes": [25, 35],
    "ingredient_prep_time_minutes": [10, 15],
    "cook_time_minutes": [15, 20],
    "servings": [3, 4],
    "ingredients": [
        "cooking spray",
        "1 medium zucchini",
        "1 large egg",
        "1/4 onion",
        "1/4 cup shredded cheese",
        "1/3 cup breadcrumbs",
        "Salt and pepper to taste"
    ],
    "equipment": [
        "Grater",
        "Spatula",
        "Kitchen towel",
        "Mixing bowl",
        "Baking sheet"
    ],
    "instructions": [
        [
            {"text": "Preheat oven to"},
            {"text": "400F", "temperature": true},
            {"text": "("},
            {"text": "200C", "temperature": true},
            {"text": "). Spray a baking sheet with cooking spray."}
        ],
        [
            {"text": "Grate the zucchini and squeeze out excess moisture using a clean kitchen towel."}
        ],
        [
            {"text": "In a bowl, combine all ingredients, and mix well until combined."}
        ],
        [
            {"text": "Spoon"},
            {"text": "1 tablespoon", "measurement": true},
            {"text": "of mixture into hands and shape into an oval. Place on the prepared baking sheet."}
        ],
        [
            {"text": "Bake"},
            {"text": "15-20", "timescale": true},
            {"text": "minutes, flipping halfway through, until golden brown and crispy."}
        ]
    ]
}