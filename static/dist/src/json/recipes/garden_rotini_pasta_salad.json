{
    "title": "Garden Rotini Pasta Salad",
    "meal_type": ["lunch", "dinner"],
    "prep_type": "no-cook",
    "total_prep_time_minutes": [25, 40],
    "ingredient_prep_time_minutes": [15, 25],
    "cook_time_minutes": [10, 15],
    "servings": [4, 6],
    "ingredients": [
        {
            "amount": "12 ounces",
            "item": "tri-color rotini"
        },
        {
            "amount": "1/2 block",
            "item": "Colby Jack Cheese"
        },
        {
            "amount": "1/2 block",
            "item": "Munster cheese"
        },
        {
            "amount": "5-6 ounce",
            "item": "black olives"
        },
        {
            "amount": "1/2 pack",
            "item": "turkey pepperoni"
        },
        {
            "amount": "1",
            "item": "red bell pepper"
        },
        {
            "amount": "15 ounces",
            "item": "canellini beans"
        },
        {
            "amount": "1/2",
            "item": "large seedless cucumber",
            "notes": [
                "or 3 mini ones"
            ]
        },
        {
            "amount": "1 bottle",
            "item": "italian dressing"
        }
    ],
    "equipment": [
        "Pot",
        "Knife",
        "Spoon",
        "Colander",
        "Cutting board",
        "Large mixing bowl"
    ],
    "instructions": [
        [
            {"text": "Boil water and cook pasta according to package instructions. Drain and rinse with cold water to cool (otherwise the cheese will melt)."}
        ],
        [
            {"text": "Cut cheese into small cubes. Cut pepperoni into halves. Dice bell pepper and cucumber. Drain and rinse beans."}
        ],
        [
            {"text": "Mix pasta, cheese, pepperoni, olives, bell pepper, cucumber, and at least half the can of beans in a large bowl."}
        ],
        [
            {"text": "Add italian dressing to taste (start with about 1/3 of bottle) and stir. Refrigerate at least a half hour before serving."}
        ]
    ]
}